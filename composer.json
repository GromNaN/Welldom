    "homepage": "https://github.com/LExpress/Welldom",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérôme Tamarelle",
            "email": "jerome@tamarelle.net"
        },
        {
            "name": "Jérôme Macias",
            "email": "jerome.macias@gmail.com"
        }
    ],
    "require": {
